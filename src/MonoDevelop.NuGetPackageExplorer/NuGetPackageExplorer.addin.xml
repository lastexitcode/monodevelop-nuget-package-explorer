<ExtensionModel>
	<Runtime>
		<Import assembly="MonoDevelop.NuGetPackageExplorer.dll" />
		<Import assembly="NuGet.exe" />
	</Runtime>

	<Extension path="/MonoDevelop/Ide/DisplayBindings">
		<DisplayBinding
			id="NuGetPackageExplorer"
			class="MonoDevelop.NuGetPackageExplorer.NuGetPackageDisplayBinding"
			insertbefore="DefaultDisplayBinding" />
	</Extension>
	
	<Extension path="/MonoDevelop/Ide/FileFilters">
		<FileFilter
			id="NuGetPackageFiles"
			_label="NuGet Package Files"
			extensions="*.nupkg" />
		<FileFilter
			id="NuSpecFiles"
			_label="NuSpec Files"
			extensions="*.nuspec" />
	</Extension>

	<Extension path="/MonoDevelop/Core/MimeTypes">
		<MimeType
			id="application/nuspec+xml"
			baseType="application/xml"
			icon="md-xml-file-icon"
			_description="NuSpec files"
			isText="true">
			<File pattern="*.nuspec" />
		</MimeType>
	</Extension>
</ExtensionModel>
